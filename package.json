{
  "name": "@gforge/react-typeahead-ts",
  "version": "0.5.0",
  "description": "A typescript rewrite of the react-typehead npm package",
  "main": "dist/typeahead.js",
  "module": "dist/typeahead.es6.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=development tsc-watch --project tsconfig.base.json  --onSuccess \"rollup -c\"",
    "prebuild": "rimraf dist",
    "build": "npm run compile",
    "build:flow": "cp src/index.js.flow dist/index.js.flow",
    "compile": "tsc -p tsconfig.base.json  && cross-env NODE_ENV=production rollup -c && cross-env NODE_ENV=development rollup -c && rimraf compiled",
    "prepare": "npm run build && npm run build:flow",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*' 'README.md'",
    "precommit": "lint-staged",
    "lint": "tslint -p .",
    "lint:fix": "tslint -p . --fix",
    "demo": "parcel serve ./demo/index.html --out-dir demo/dist",
    "build:demo": "parcel build demo/index.html --out-dir demo/dist --public-url=/react-typeahead-ts",
    "deploy": "npm run build:demo && gh-pages -d demo/dist",
    "reset:type": "rimraf nodemodules/@types && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gforge/react-typeahead-ts.git"
  },
  "keywords": [
    "reactstrap",
    "typeahead",
    "react",
    "component"
  ],
  "author": "Max Gordon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gforge/react-typeahead-ts/issues"
  },
  "homepage": "https://gforge.github.com/react-typeahead-ts",
  "dependencies": {
    "classnames": "^2.2.6",
    "fuzzy": "^0.1.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.13",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.116",
    "@types/react": "^16.4.7",
    "@types/react-dom": "^16.0.6",
    "@types/reactstrap": "^8.0.1",
    "@types/sinon": "^7.0.13",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "bootstrap": "^4.1.3",
    "create-react-class": "^15.6.3",
    "cross-env": "^5.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-typescript": "^4.0.0",
    "eslint-config-prettier": "^5.0.0",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "flowgen": "^1.2.2",
    "formik": "^1.0.2",
    "gh-pages": "^2.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
    "lint-staged": "8.2.1",
    "lodash": "^4.17.10",
    "parcel": "^1.9.7",
    "prettier": "1.18.2",
    "raf": "^3.4.0",
    "react": "^16.4",
    "react-dom": "^16.4",
    "react-syntax-highlighter": "^10.3.0",
    "react-test-renderer": "^16.4.2",
    "reactstrap": "^8.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.15.6",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-filesize": "6.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "5.0.3",
    "rollup-plugin-replace": "2.2.0",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-terser": "^5.0.0",
    "sinon": "^7.3.2",
    "ts-jest": "^24.0.2",
    "tsc-watch": "2.2.1",
    "tslib": "^1.9.3",
    "typescript": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^16.x",
    "react-dom": "^16.x"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFiles": [
      "raf/polyfill",
      "<rootDir>/test/setupTest.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/?(*.)(spec|test).(ts|js)?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  }
}
